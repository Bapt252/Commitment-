          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  CASE\n    WHEN me.match_score >= 90 THEN 'Excellent (90-100)'\n    WHEN me.match_score >= 80 THEN 'Très bon (80-89)'\n    WHEN me.match_score >= 70 THEN 'Bon (70-79)'\n    WHEN me.match_score >= 50 THEN 'Moyen (50-69)'\n    ELSE 'Faible (<50)'\n  END as \"Qualité du match\",\n  COUNT(*) as \"Nombre\"\nFROM tracking.events e\nJOIN tracking.match_events me ON e.event_id = me.event_id\nWHERE e.event_type = 'match_proposed'\nAND e.event_timestamp >= NOW() - INTERVAL '30 days'\nGROUP BY \"Qualité du match\"\nORDER BY MIN(me.match_score) DESC",
