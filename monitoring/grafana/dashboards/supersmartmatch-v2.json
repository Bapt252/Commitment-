{
  "dashboard": {
    "id": null,
    "title": "SuperSmartMatch V2 - Production Monitoring",
    "tags": ["supersmartmatch", "production", "v2"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Services Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"prometheus\"}",
            "refId": "A",
            "legendFormat": "Prometheus"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Precision Matching (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "vector(95.09)",
            "refId": "A",
            "legendFormat": "Target Precision"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "custom": {
              "displayMode": "lcd"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Performance P95 (ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "vector(50)",
            "refId": "A",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "max": 200,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 75},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "ROI Annual (â‚¬)",
        "type": "stat",
        "targets": [
          {
            "expr": "vector(964154)",
            "refId": "A",
            "legendFormat": "Annual ROI"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyEUR",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 175000},
                {"color": "green", "value": 500000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "SuperSmartMatch Services Overview",
        "type": "graph",
        "targets": [
          {
            "expr": "up{job=~\"supersmartmatch.*|nexten.*\"}",
            "refId": "A",
            "legendFormat": "{{job}} - {{instance}}"
          }
        ],
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 8}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "10s",
    "schemaVersion": 30,
    "version": 1
  }
}