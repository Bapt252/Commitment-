
# Configuration OpenAI (REQUIS)
# Vous devez fournir votre clé API OpenAI
OPENAI=votre_clé_api_openai_ici

# Configuration optionnelle du service cv-parser
# Modèle OpenAI à utiliser (gpt-4o-mini recommandé pour parsing CV)
OPENAI_MODEL=gpt-4o-mini

# Désactiver le mode mock parser (recommandé)
USE_MOCK_PARSER=false

# Paramètres Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# Configuration MinIO
MINIO_ENDPOINT=storage:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET_NAME=cv-files

# Configuration des ports
PORT=5000

# Configuration de debug
DEBUG=false
LOG_LEVEL=INFO
